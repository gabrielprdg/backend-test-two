{
  "name": "backend-test-two",
  "version": "1.0.0",
  "description": "## SITUAÇÃO-PROBLEMA",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests --silent --noStackTrace",
    "test:staged": "npm test -- --findRelatedTests",
    "test:coverage": "npm test -- --coverage",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "start:dev": "ts-node src/main/server.ts",
    "start:prod": "node dist/main/server.js",
    "watch": "node --inspect=0.0.0.0:9222 --nolazy dist/main/server.js",
    "dev": "sucrase-node src/main/server.ts",
    "up": "sudo docker compose up",
    "down": "sudo docker compose down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielprdg/backend-test-two.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gabrielprdg/backend-test-two/issues"
  },
  "homepage": "https://github.com/gabrielprdg/backend-test-two#readme",
  "dependencies": {
    "express": "^4.18.2",
    "mongodb": "5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "git-commit-msg-linter": "^4.5.0",
    "jest": "^29.4.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
